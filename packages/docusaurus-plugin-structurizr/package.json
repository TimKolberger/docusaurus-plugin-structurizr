{
  "name": "docusaurus-plugin-structurizr",
  "description": "This plugin allows you to use structurizr diagrams in your docusaurus documentation.",
  "keywords": [
    "docusaurus",
    "docusaurus-plugin",
    "documentation",
    "diagrams",
    "structurizr",
    "mermaid",
    "plantuml"
  ],
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TimKolberger/docusaurus-plugin-structurizr.git",
    "directory": "packages/docusaurus-plugin-structurizr"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "concurrently pnpm:lint:*",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:format": "prettier --check --config ../../package.json .",
    "lint:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@docusaurus/types": "3.0.1",
    "@dps/eslint-config": "workspace:*",
    "@dps/typescript-config": "workspace:*",
    "@types/node": "20.10.5",
    "concurrently": "8.2.2",
    "eslint": "^8.53.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@docusaurus/utils-validation": "^3.0.1",
    "globby": "^14.0.0"
  }
}
